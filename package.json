{
  "private": true,
  "name": "starter-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules coverage reports public .cache",
    "deploy": "spraoi deploy-ui -c $config",
    "format": "yarn svgo && yarn prettier",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.{js,gql,json,md,yml}\"",
    "serve": "gatsby serve --port 8000",
    "start": "gatsby develop",
    "svgo": "GLOBIGNORE=*limra-logo.svg && svgo --config node_modules/@spraoi/svgo-config/svgo.yml src/images/**/*.svg",
    "test": "jest --ci --runInBand"
  },
  "dependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@reach/router": "^1.3.4",
    "@spraoi/auth": "^0.29.5",
    "@spraoi/babel-preset": "^0.29.0",
    "@spraoi/base": "^0.29.19",
    "@spraoi/gatsby-config": "^0.29.23",
    "@spraoi/helpers": "^0.29.0",
    "@spraoi/scripts": "^0.29.20",
    "@spraoi/svgo-config": "^0.29.0",
    "@spraoi/validations": "^0.29.0",
    "@types/react": "^16.9.46",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.5.5",
    "final-form": "^4.20.1",
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-extract-image-colors": "^0.0.14",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-sitemap": "^2.2.6",
    "gatsby-remark-dropcap": "^0.1.2",
    "gatsby-remark-images": "^3.3.25",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
    "gatsby-remark-prismjs": "^3.3.5",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-remark": "^2.8.28",
    "gatsby-transformer-sharp": "^2.5.13",
    "gatsby-transformer-yaml": "^2.2.4",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.3",
    "lodash": "^4.17.20",
    "polished": "^3.6.5",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-burgers": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.0",
    "react-final-form-listeners": "^1.0.2",
    "react-helmet": "^6.1.0",
    "react-simple-maps": "^2.1.2",
    "react-social-sharing": "^3.3.0",
    "react-spinners": "^0.9.0",
    "styled-components": "^5.1.1",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.29.0",
    "@spraoi/jest-config": "^0.29.1",
    "@spraoi/prettier-config": "^0.29.0",
    "@spraoi/stylelint-config": "^0.29.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "jest-haste-map": "^26.3.0",
    "jest-junit": "^11.1.0",
    "jest-resolve": "^26.4.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1"
  }
}
